{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "exclude": [
    ],
    "includePattern": ".+\\.ts(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component"],
  "templates": {
    "search": true,
    "better-docs": {
      "name": "Hacker News",
      "navLinks": [
        {
          "label": "GitHub",
          "href": "https://github.com/cmarinkovic/hacker-news"
        },
        {
          "label": "Deployed Application",
          "href": "https://festive-lewin-d0e894.netlify.app/"
        }
      ]
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "verbose": true,
    "template": "./node_modules/better-docs"
  }
}
